datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

generator prismaClassGenerator {
    provider   = "prisma-class-generator"
    useSwagger = true
    dryRun     = false
}

model User {
    id        String   @id @default(uuid())
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    email    String @unique
    password String
}
